int maxArea(int* height, int heightSize) {
    if(heightSize<=1)
    return 0;

    int left=0, right=heightSize-1 , max =0;
    while(left < right)
    {
        int length = fmin(height[left],height[right]);
        int width = right - left;
        int area = length * width;
        max = fmax(area,max);
        height[left]<height[right]?left++:right--;
    }
    return max;
}
