int removeElement(int* nums, int numsSize, int val) 
{
    int hash[101]={0};

    for(int i=0;i<numsSize;i++)
    {
        hash[nums[i]]++;
    }

    int j=0;
    for(int i=0;i<101;i++)
    {
        if(i==val)  continue;

        while(hash[i])
        {
            nums[j++]=i;
            hash[i]--;
        }
    }
    return j;
}
