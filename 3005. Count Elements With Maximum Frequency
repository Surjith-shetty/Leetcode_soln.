int maxFrequencyElements(int* nums, int numsSize)
{

    if(numsSize==1)
    {
        return 1;
    }

    int arr[102]={0};

    for(int i=0;i<numsSize;i++)
    {
        arr[nums[i]]++;
    }    


    int max=0;
    int count;

    for(int i=0;i<102;i++)
    {
        if(arr[i]>max)
        {
            max=arr[i];
        }
    }

    count=0;
    for(int i=0;i<102;i++)
    {
        if(arr[i]==max)
        {
            count=count+arr[i];
        }
    }

    return count ;
}
