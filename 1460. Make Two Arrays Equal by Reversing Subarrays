bool canBeEqual(int* target, int targetSize, int* arr, int arrSize) 
{
    int hash[1001]={0};
    int hash1[1001]={0};

    for(int i=0;i<targetSize;i++)
    {
        hash[target[i]]++;
    }

    for(int i=0;i<arrSize;i++)
    {
        hash1[arr[i]]++;
    }

    for(int i=0;i<1001;i++)
    {
        if(hash[i]!=hash1[i])
        return false;
    }
    return true;
}
