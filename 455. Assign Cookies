//java

class Solution {
    public int findContentChildren(int[] g, int[] s) 
    {
        Arrays.sort(g);
        Arrays.sort(s);
        int i,j;
        int satisfied =0 ;
        for(i=(g.length-1),j=(s.length-1);i>=0 && j>=0;)
        {
            if(s[j]>=g[i])
            {
                satisfied++;
                i--;
                j--;
            }
            else
            {
                i--;
            }
        }
        return satisfied;
    }
}
