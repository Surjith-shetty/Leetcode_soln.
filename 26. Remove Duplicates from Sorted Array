int removeDuplicates(int* nums, int numsSize) 
{
    if(numsSize==0)
    return 0;

    int i,j;
    for(i=0,j=0;j<numsSize;)
    {
        if(nums[i]==nums[j])
        {
            j++;
            continue;
        }
        else
        {
            i++;
            nums[i]=nums[j];
        }

    }
    return i+1;
}
